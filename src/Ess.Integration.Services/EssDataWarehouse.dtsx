                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	 0					AS ProdutoId
	,'INDEFINIDO'		AS Produto
	,'0'				AS FornecedorId
	,'INDEFINIDO'		AS Fornecedor 
	,'0'				AS CategoriaId
